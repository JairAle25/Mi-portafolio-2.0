---
import Links from "../constants/LinksHeader"
---



<footer class="rounded-lg shadow mt-48 pb-6">
    <div class="w-full max-w-screen-xl mx-auto p-4 md:py-8">
        <div class="xl:flex xl:flex-row sm:flex sm:items-center sm:justify-between md:flex md:flex-col md:gap-3">
            <a href="https://portafolio-jair.netlify.app/" class="flex items-center mb-4 sm:mb-0 space-x-3 rtl:space-x-reverse">
                <img src="/logoFooter.png" class="w-14" alt="Flowbite Logo" />
                <span class="self-center text-2xl font-semibold whitespace-nowrap dark:text-white">JairC</span>
            </a>
            <ul class="flex flex-wrap items-center mb-6 text-base font-medium text-gray-500 sm:mb-0 dark:text-gray-400">
                {Links.map(link => <li><a href={link.link} class="hover:underline me-4 md:me-6">{link.name}</a></li>)}
            </ul>
        </div>
        <hr class="my-6 border-gray-200 sm:mx-auto dark:border-gray-700 lg:my-8" />
        <span class="block text-sm text-gray-500 sm:text-center dark:text-gray-400">© 2024 <a href="https://portafolio-jair.netlify.app/" class="hover:underline">JairC™</a>. All Rights Reserved.</span>
    </div>
</footer>

